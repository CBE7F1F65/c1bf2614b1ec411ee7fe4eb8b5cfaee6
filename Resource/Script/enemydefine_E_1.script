///////////// BOSS /////////////////
# 0x0180	//CW白血

@ 1

EA 2 40
	60

@ @@[]% 72 156 12

SD [i] EN@ / 12 - 5
SD [tAngle] i - 1 * 1000
SD [angleOffset] 9000 - tAngle

LOOP [i]
{
	SD [angle] i * 2000 + angleOffset
	A 10
		TIMERRANGE 60 120 ANGLESETADD 9000 - angle / 20
		TIMERRANGE 60 120 SPEEDSETADD 2
	B ENX ENY
		true angle 1.0
		8 3 8 0	0xff
}
A 0

@ 160

EA 4 42
	60 120 280

@ @@[]% 225 390 15

SD [timeLess] EN@ < 280
SD [timeOver] EN@ > 340
IF timeLess + timeOver
{
	A 8
		TIMERLESS 120 ANGLESETADD SIGN timeLess * 400
		TIMERLESS 120 SPEEDSETADD 1
	BC 8 true
		ENX ENY 1.0
		0 4 8 0
	A 0
}

@ 280

EA 4 42
	60 320 280

@ 400

EA 2 40
	60

@ 480

SD [i] 4
LOOP [i]
{
	GB 0x8106 0xff ENI
		ENX ENY 9000 * i 3.0
		0 5 80
}

