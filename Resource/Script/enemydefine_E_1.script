# 0x0190	//FXRANGE异想天开

@ 1

EA 2 40
	60
	
@ 120

SD [randAngle] RANDA
SD [i] 6
LOOP [i]
{
	SD [angle] i > 2 * 18000
	SD [angle] i * 12000 + angle + randAngle
	GB 0x8152 0xff ENI
		COSA angle *f 80.0 +f ENX SINA angle *f 80.0 +f ENY angle 0.0
		0 40 80
	SD [aimAngle] angle + 15000
	GHSAIM COSA aimAngle SINA aimAngle
}