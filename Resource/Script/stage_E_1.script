@ @@>]% 160 760 8
//四波探子
//（AIMX是否发射）
IF TIME % 180 < 32
{
	SD [xOffset] TIME % 360 < 180 * 240
	EB 0x0100 0xff
		RAND % 100 + 40 + xOffset 0 9000 3.0
		0 1 16 ENI % 3 > 1 * 15 + 1
}

@ 780
//主攻带使魔小兵
EB 0x0102 0xff
	120 0 9000 4.0
	1 5 16 0x00000022

@ @@>]% 820 880 15
//四个带使魔增援
EB 0x0104 0xff
	320 0 9000 2.0
	1 1 16 0x00000011

@ @@>]% 960 1440 120
//四个主攻带使魔小兵
//（AIMX携带使魔数量）
SD [xBase] TIME / 120 - 9
EB 0x0106 0xff
	60 * xBase + 130 0 9000 5.0
	3 8 16 ENI % 2 + 16
ENSAIM xBase / 2 * 2 + 2 0

@ @@>]% 1620 1780 15
//自杀性小兵
EB 0x0100 0xff
	RAND % 440 0 9000 5.0
	1 1 16 0x00000001

@ 1840
//使魔延迟
SD [i] 2
LOOP [i]
{
	EB 0x0108 0xff
		220 0 i * -6000 + 12000 3.0
		1 8 16 0x00000121
}

@ @@[]% 1900 2020 20
//冲上型小兵
//（AIMX子弹旋转方向）
SD [i] 2
LOOP [i]
{
	EB 0x0110 0xff
		i * 200 + 120 480 -9000 10.0
		2 1 70 ENI % 3 > 1 + 16
	ENSAIM SIGN i 0
}

//Repeat

@ 2220
//主攻带使魔小兵
EB 0x0102 0xff
	320 0 9000 4.0
	1 10 16 0x00000031

@ @@>]% 2280 2340 15
//四个带使魔增援
