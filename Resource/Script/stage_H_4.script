# 402	//演示

@ @@> 0

EXECUTE CT_STAGE 400 TIME

@ @@> 270

SD [isWushuang] CHARA - 3 !
IF CHATI - 0xff ! + isWushuang
{
	SETSCENE 406
}

/////////////
# 406	//道中1

@ 1

BGSETUP 1 0x0402 true
BGSETUP 4 0x0440 true

@ 240

BGSETUP 1 0 true

@ @@[ 180

CALL $ST3BTrap TIME

//拉线
@ @@[]% 400 1600 400

EB 0x2400 0xff
	TIME % 800 < 400 * 100 + 170 0 9000 2.0
	1 5 300 0x00000152

//拦截
@ @@[]% 400 2000 120

SD [bRight] TIME % 800 < 400 !
SD [randX] RAND % 200 - 100
EB 0x2402 0xff
	bRight * 200 + 120 0 9000 5.0
	0 5 80 0x00000042
ENSAIM bRight 0

//主攻
@ 2000

EB 0x2404 0xff
	220 0 9000 2.5
	3 20 360 0x000000A5

//排列
@ @@[]% 2600 2780 20

EXECUTE CT_STAGE 404 TIME

@ 3060

SETTIME 0
SETSCENE 410

